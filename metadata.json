{
  "name": "puppet-dropbear",
  "version": "1.1.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Puppet dropbear module",
  "license": "GPL-3.0",
  "source": "git://github.com/voxpupuli/puppet-dropbear.git",
  "project_page": "https://github.com/voxpupuli/puppet-dropbear/",
  "issues_url": "https://github.com/voxpupuli/puppet-dropbear/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.10.0 < 6.0.0"
    }
  ],
  "description": "Module to install/manage Dropbear SSH server using puppet",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.0.0 <5.0.0"
    }
  ]
}
